<?xml version="1.0"?>
<transfers
        xmlns="spryker:databuilder-01"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="spryker:databuilder-01 http://static.spryker.com/databuilder-01.xsd"
>
    <transfer name="ProductOfferStock">
        <property name="quantity" dataBuilderRule="randomNumber(5)"/>
        <property name="productOffer" dataBuilderRule="ProductOffer"/>
        <property name="stock" dataBuilderRule="Stock"/>
        <property name="isNeverOutOfStock" dataBuilderRule="boolean(0)"/>
    </transfer>
</transfers>
